<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppIntegradora10A.Views.EditProductPage"
             Title="EditProductPage">
    <StackLayout Padding="20">
        <Label Text="Editar Reporte" FontSize="24" HorizontalOptions="Center"/>
        <Entry x:Name="NombreEntry" Placeholder="Que tipo de incidencia presenta"/>
        <Entry x:Name="DescripcionEntry" Placeholder="Descripcion de la incidencia"/>
        <Entry x:Name="PrecioEntry" Placeholder="Codigo numero del error que presenta" Keyboard="Numeric"/>
        <Button Text="Guardar Cambios" x:Name="BtnOnEditProducto" Clicked="BtnOnEditProducto_Clicked"/>

    </StackLayout>


</ContentPage>